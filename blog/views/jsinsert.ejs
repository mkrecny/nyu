<html>
  <head>
  </head>
<script type='text/javascript'>
    var tags = [
      {name:'card',
      content:'summary'},
      {name:'site',
      content:'@recborg'},
      {name:'creator',
      content:'@recborg'},
      {name:'title',
      content:'Insertion'},
      {name:'description',
      content:'This is a test description for dynamic JS insertion of meta tags'},
      {name:'domain',
      content:'myles.io'},
      {name:'image:src',
      content:'http://cdn.frontpagemag.com/wp-content/uploads/2015/04/javascript.png'},
      {name:'url',
      content:'http://myles.io/thoughts/jsinsert'},
      {name:'app:name:iphone',
      content:''},
      {name:'app:name:ipad',
      content:''},
      {name:'app:name:googleplay',
      content:''},
      {name:'app:url:iphone',
      content:''},
      {name:'app:url:googleplay',
      content:''},
      {name:'app:id:iphone',
      content:''},
      {name:'app:url:ipad',
      content:''},
      {name:'app:id:ipad',
      content:''},
      {name:'app:id:googleplay',
      content:''}
    ];
    tags.forEach(function(tag){
      console.log('inserting', tag);
      var meta = document.createElement('meta');
      meta.setAttribute('name', 'twitter:'+tag.name);
      meta.setAttribute('content', tag.content);
      document.getElementsByTagName('head')[0].appendChild(meta);
    });
</script>
<body></body>
  

  <div class="header">
    JS insertion test 01.      
  </div>

  <style>
      .header{
        font-size:22px;
        padding:20px;
        width:100%;
      }
      iframe{
        width:100%;
        height:1000px;
      }
      body{
        overflow-x:hidden;
        overflow-y:hidden;
      }
    </style>
</html>


